{
  "extends": [
    "standard",
    "plugin:react/recommended"
  ],
  "plugins": [
    "standard",
    "promise",
    "react"
  ],
  "rules": {
    "space-in-parens": "off",
    "space-before-function-paren": ["error", {
      "anonymous": "always",
      "named": "never",
      "asyncArrow": "always"
    }],
    "react/prop-types": "off",
    "spaced-comment": "off"
  }
}
